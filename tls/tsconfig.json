{
  "compilerOptions":{
    "outDir":"./dist/",
    "noImplicitAny": true,
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports":true,
    "experimentalDecorators":true,
    "emitDecoratorMetadata":true,
    "sourceMap":false,
    "baseUrl": ".",
    "strict": true,
    "paths": {
      "@/*": ["render/*"],
      "~/*": ["main/*"],
    }
  },
  "exclude": [
    "第三方"
  ]
}